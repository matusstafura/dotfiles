alias ar="service apache2 restart"
alias hm="cd /data/web/syndicate/public_html"
alias dev="php -f /data/web/syndicate/public_html/bin/magento deploy:mode:set developer"
alias prd="php -f /data/web/syndicate/public_html/bin/magento deploy:mode:set production"
alias fls="php /data/web/syndicate/public_html/bin/magento cache:flush"
alias ixs="php /data/web/syndicate/public_html/bin/magento indexer:reindex"
alias dpl="php /data/web/syndicate/public_html/bin/magento setup:static-content:deploy"
alias fix="chmod -R 777 /data/web/syndicate/public_html/var /data/web/syndicate/public_html/generated /data/web/syndicate/public_html/pub"
alias dbb="bash /backup/backup_mysql.sh"
alias css="vim /data/web/syndicate/public_html/app/design/frontend/Svart/Svan/web/css/theme.css"
alias trs="php /data/web/syndicate/public_html/bin/magento i18n:pack --mode=replace --allow-duplicates sk_SK.csv sk_SK && fls"
alias trc="php /data/web/syndicate/public_html/bin/magento i18n:pack --mode=replace --allow-duplicates cs_CZ.csv cs_CZ && fls"
alias upg="php /data/web/syndicate/public_html/bin/magento setup:upgrade && fix"
alias chm="chown -R matus:www-data /data/web/syndicate/"
alias all="chm && flush && ix"
alias n98="n98-magerun2.phar"
